  <div class="col d-flex justify-content-center h6">Mental Health Score</div>  
  <div class="col d-flex justify-content-center h6">Physical Health Score</div>    
</div>
<div class="row mb-5">
  <div class="col d-flex justify-content-center h6 fw-bold"><%= number_with_delimiter object.bioscore, delimiter: ',' %></div>
